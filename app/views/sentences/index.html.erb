<p id="notice"><%= notice %></p>

<h1>Listing Sentences</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>Chapter</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sentences.each do |sentence| %>
      <tr>
        <td><%= sentence.content %></td>
        <td><%= sentence.chapter_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


